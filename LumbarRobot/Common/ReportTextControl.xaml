<UserControl x:Class="LumbarRobot.Common.ReportTextControl"
            xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"              
             mc:Ignorable="d"  Focusable="True" 
             d:DesignHeight="300" d:DesignWidth="300" BorderThickness="1" SizeChanged="UserControl_SizeChanged" >
    <Grid Name="GridText" Margin="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
        <Line Grid.RowSpan="1" Grid.ColumnSpan="1" Stroke="Black" StrokeThickness="1" Name="TopFrame" VerticalAlignment="Top" HorizontalAlignment="Left" />
        <Line Grid.RowSpan="1" Grid.ColumnSpan="1" Stroke="Black" StrokeThickness="1" Name="LeftFrame" X1="1" X2="1" Y1="0" Y2="20" VerticalAlignment="Top" HorizontalAlignment="Left"/>
        <Line Grid.RowSpan="1" Grid.ColumnSpan="1" Stroke="Black" StrokeThickness="1" Name="RightFrame" VerticalAlignment="Top" HorizontalAlignment="Right"/>
        <Line Grid.RowSpan="1" Grid.ColumnSpan="1" Stroke="Black" StrokeThickness="1" Name="BottomFrame" VerticalAlignment="Bottom" HorizontalAlignment="Left"/>
    </Grid>
</UserControl>

