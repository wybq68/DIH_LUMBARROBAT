<UserControl x:Class="LumbarRobot.ActionUserControl.SinControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:wf="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <DockPanel Name="dockplan" LastChildFill="True">
        <WindowsFormsHost Canvas.Left="0" Canvas.Top="0"   Name="windowsFormsHost1"  DockPanel.Dock="Top" >
            <wf:PictureBox x:Name="picback" Paint="picback_Paint"></wf:PictureBox>
        </WindowsFormsHost>
    </DockPanel>
</UserControl>
