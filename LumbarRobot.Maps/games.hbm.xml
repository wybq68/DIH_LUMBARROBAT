<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="LumbarRobot.Data.Games, LumbarRobot.Data" table="games">
    <id name="Id" type="Int32" unsaved-value="0">
      <column name="GameId" sql-type="int" not-null="true" unique="true"/>
      <generator class="assigned" />

    </id>
    <property name="GameName" type="String">
      <column name="GameName" length="135" sql-type="varchar" not-null="true"/>
    </property>
    <property name="GameIco" type="String">
      <column name="GameIco" length="135" sql-type="varchar" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
